<%- include("partials/header"); -%>
    <!-- Main-Content-->
    <section class="admin">
        <div class="container-fluid">
            <div class="row mt-3">
                <div class="col-lg-3 col-md-3">
                    <div class="list-group mb-3">
                        <div class="list-group-item active">Interns Data</div>
                        <a href="/" class="list-group-item ">Add
                            Intern</a>
                        <a href="/admin" class="list-group-item">View All</a>
                    </div>
                </div>
                <div class="col-lg-9 col-md-9 table-responsive">
                    <table class="table table-striped bg-light small">
                        <tr class="table-dark">
                            <th>Name</th>
                            <th>Email</th>
                            <th>Mobile No.</th>
                            <th>WhatsApp No.</th>
                            <th>College Name</th>
                            <th>Refrence</th>
                            <th>Qualification</th>
                            <th>Intenship Type</th>
                            <th>CV</th>
                        </tr>
                        <% entries.forEach((entry)=>{ %>
                            <% console.log(entry); %>
                            <tr>
                                <td> <%= entry.name %> </td>
                                <td> <%= entry.email %> </td>
                                <td> <%= entry.contact %> </td>
                                <td> <%= entry.whatsapp %> </td>
                                <td> <%= entry.college %> </td>
                                <td> <%= entry.refrence %> </td>
                                <td> <%= entry.degree %> </td>
                                <td> <%= entry.intern %> </td>
                                <td> <a href="<%= entry.file %>"><i class="fa fa-file-pdf-o" aria-hidden="true"></i></a> </td>
                            </tr>        
                        <% }); %>
                                
                    </table>
                </div>
            </div>
        </div>
        <!-- Main-Content-->
    </section>    
<%- include("partials/footer"); -%>